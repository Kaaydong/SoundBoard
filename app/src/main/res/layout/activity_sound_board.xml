<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SoundBoardActivity">

    <Button
        android:id="@+id/button_main_Fsh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/fsh"
        app:layout_constraintBottom_toTopOf="@+id/button_main_Gsh"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.386"
        app:layout_constraintStart_toEndOf="@+id/button_main_F"
        app:layout_constraintTop_toBottomOf="@+id/button_main_Dsh" />

    <Button
        android:id="@+id/button_main_Csh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Csh"
        app:layout_constraintBottom_toTopOf="@+id/button_main_Dsh"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.383"
        app:layout_constraintStart_toEndOf="@+id/button_main_C"
        app:layout_constraintTop_toBottomOf="@+id/button_main_Bfl" />

    <Button
        android:id="@+id/button_main_Bfl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="98dp"
        android:text="@string/Bfl"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.383"
        app:layout_constraintStart_toEndOf="@+id/button_main_B"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_main_a"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="293dp"
        android:text="@string/a"
        app:layout_constraintBottom_toTopOf="@+id/button_main_B"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_main_B"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="29dp"
        android:layout_marginEnd="294dp"
        android:text="@string/b"
        app:layout_constraintBottom_toTopOf="@+id/button_main_C"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_main_a" />

    <Button
        android:id="@+id/button_main_Dsh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/d"
